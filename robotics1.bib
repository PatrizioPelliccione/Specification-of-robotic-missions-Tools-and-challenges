file = {:C$\backslash$:/Users/dragule/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/G{\"{o}}tz et al. - 2012 - A role-based language for collaborative robot applications.pdf:pdf},
isbn = {9783642347801},
issn = {18650929},
journal = {Communications in Computer and Information Science},
number = {1},
pages = {1--15},
title = {{A role-based language for collaborative robot applications}},
volume = {336 CCIS},
year = {2012}
}
@article{Gherardi2015,
abstract = {Robotics applications are characterized by a huge amount of variability. Their design requires the developers to choose between several variants, which relate to both functionalities and hardware. Some of these choices can be taken at deployment-time, however others should be taken at run-time, when more information about the context is known. To make this possible, a software system needs to be able to reason about its current state and to adapt its architecture to provide the configuration that best suites the context. This paper presents a model-based approach for run-time adaptation of robotic systems. It defines a set of orthogonal models that represent the system architecture, its variability, and the state of the context. Additionally it introduces a set of algorithms that reason about the knowledge represented in our models to resolve the run-time variability and to adapt the system architecture. The paper discusses and evaluates the approach by means of two case studies.},
annote = {In the RRA approach, adaptation and the run-time variability resolutions are performed on the model and then applied to the real system since Deployment-time Configured System Model is stored in the Run-time Configured System Model. After the initialization the Run-time Configured System Model is an exact copy of the Deployment-time Configured System Model. Much as RRA approach can enable robots to adapt models at run time, the configurations are made by an expert programmer, but not domain expert declaratively.},
author = {Gherardi, Luca and Hochgeschwender, Nico},
doi = {10.1109/IROS.2015.7353608},
file = {:C$\backslash$:/Users/dragule/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gherardi, Hochgeschwender - 2015 - RRA Models and tools for robotics run-time adaptation.pdf:pdf},
isbn = {9781479999941},
issn = {21530866},
journal = {IEEE International Conference on Intelligent Robots and Systems},
keywords = {Adaptation models,Computer architecture,Context,Context modeling,Robots,Software,Software algorithms},
pages = {1777--1784},
title = {{RRA: Models and tools for robotics run-time adaptation}},
volume = {2015-Decem},
year = {2015}
}

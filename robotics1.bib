@article{Baker2006,
abstract = {aker, L. (2006). Observation: A Complex Research Method. Library Trends, 55(1), 171–189. doi:10.1353/lib.2006.0045 (Baker, 2006) Observation as a research method has been called the “bedrock source of human knowledge” by Adler and Adler (1994). Cited as being used by ancient researchers including Aristotle and Auguste Comte, observation has been used for as long as human history has been recorded in an attempt to understand the world. In modern times, observation as a research method has become nebulous in definition. A variety of labels referring to observation as a research method seem to be used interchangeably by researchers including observation, participant observation, and ethnography. Within these broad terms for observation there are several roles a researcher may take depending on the study ranging from nonparticipation to complete membership in the group being studied. Nonparticipation is at one end of the continuum of roles a researcher may play in observation as a research method. In this role, the researcher has no involvement with the participants of the research. The researcher may analyze data about people or watch groups and individuals via camera. The individuals being observed may or may not be aware of the researcher. Slightly more involved with the participants than the non-participatory researcher is the complete observer. In this role, the researcher is present with the individuals being studied, but takes special care to detach from the group. This role is limited in value due to lack of access to ‘insider' information. It is, however, a good starting point to collect baseline data on a group. An observer as participant is still primarily a researcher and so maintains a certain detachment from the group, but interacts with participants through interviews and conversations. In this role, the participants are aware of the researcher's presence in the group. More popular with qualitative researchers is the role of moderate or peripheral membership. This role involves a balance between insider and outsider. The researcher limits their involvement so as not to influence the specific activities being studied. In some cases, it is advantageous for the researcher to gain active membership in the group. In this role, the researcher participates in the activities of the group and even develops relationships with group members. This can be necessary when the researcher needs to understand intimate information or intricate details of an activity. Finally, complete participation is at the complete opposite end of the continuum from nonparticipation. In this role, the researcher acts as a member and not primarily as a researcher. This role gives the researcher a thorough and intimate understanding of the group, but can become too involved to remain objective. Adler, P. A., {\&} Adler, P. (1994). Observational techniques. In N. K. Denzin {\&} Y. S. Lincoln (Eds.), Handbook of qualitative research (pp. 377–392). Thousand Oaks, CA: Sage Publica- tions. Baker, L. (2006). Observation: A Complex Research Method. Library Trends, 55(1), 171–189. doi:10.1353/lib.2006.0045},
author = {Baker, Lynda},
doi = {10.1353/lib.2006.0045},
file = {:C$\backslash$:/Users/dragule/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Baker - 2006 - Observation A Complex Research Method.pdf:pdf},
isbn = {00242594},
issn = {1559-0682},
journal = {Library Trends},
number = {1},
pages = {171--189},
pmid = {220467949},
title = {{Observation: A Complex Research Method}},
volume = {55},
year = {2006}
}
@article{Baskerville2015,
abstract = {Until recently, most ethnographic research in information systems has been based on the traditional anthropological model of ethnography. In this traditional model of ethnography, one of the most important data-gathering techniques is participant observation. The ethnographer observes and participates but does not actively seek to change the situation. In recent years, however, a new type of ethnographic research has emerged – one that can be called design ethnography. Design ethnography is where the researcher goes beyond observation and actively engages with people in the field. Our view is that design ethnography has much potential for research in information systems. It is a new form of engaged scholarship that bridges the gap between ethnographic research and design science research.},

issn = {0929-5593, 1573-7527},
journal = {Autonomous Robots},
keywords = {Artificial Intelligence (incl. Robotics),Automation and Robotics,Computer Imaging,Electronic and Computer Engineering,Graphics and Computer Vision,Mechanical Engineering,autonomous robotics,mission specification,visual programming},
number = {1},
pages = {29--52},
publisher = {Kluwer Academic Publishers},
title = {{Multiagent Mission Specification and Execution}},
url = {https://www.cc.gatech.edu/ai/robot-lab/tmr/mission-auro.pdf http://link.springer.com/article/10.1023/A:1008807102993},

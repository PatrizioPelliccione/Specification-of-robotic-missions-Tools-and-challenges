@article{Magnenat2011,
abstract = {We propose ASEBA, a modular architecture for event-based control of complex robots. ASEBA runs scripts inside virtual machines on self-contained sensor and actuator nodes. This distributes processing with no loss of versatility and provides several benefits. The closeness to the hardware allows fast reactivity to environmental stimuli. The exploitation of peripheral processing power to filter raw data offloads any central computer and thus allows the integration of a large number of peripherals. Due to scriptable and plug-and-play modules, ASEBA provides instant compilation and real-time monitoring and debugging of the behavior of the robots. Our results show that ASEBA improves the performance of the behavior with respect to other architectures. For instance, doing obstacle avoidance on the marXbot robot consumes two orders of magnitude less bandwidth than using a polling-based architecture. Moreover, latency is reduced by a factor of two to three. Our results also show how ASEBA enables advanced behavior in demanding environments using a complex robot, such as the handbot robot climbing a shelf to retrieve a book.},
author = {Magnenat, St{\'{e}}phane and R{\'{e}}tornaz, Philippe and Bonani, Michael and Longchamp, Valentin and Mondada, Francesco},
doi = {10.1109/TMECH.2010.2042722},
file = {:E$\backslash$:/mak sida317 lab005 june 2018/SIDA BRIGHT 317/New research direction/aseba thymio/aseba-ieee-tr a modular architecture for event based control of complext robots.pdf:pdf},
issn = {10834435},
journal = {IEEE/ASME Transactions on Mechatronics},
keywords = {Intelligent actuators,intelligent sensors,microcontrollers,mobile robots},
mendeley-groups = {surveys robot DE},
number = {2},
pages = {321--329},
title = {{ASEBA: A modular architecture for event-based control of complex robots}},
volume = {16},
year = {2011}
}

